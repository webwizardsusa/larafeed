{
  "name": "webwizardsusa/larafeed",
  "description": "Generate rss feeds",
  "keywords": [
    "rss",
    "laravel",
    "larafeed",
    "webwizardsusa"
  ],
  "homepage": "https://webwizardsusa.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamie Holly",
      "email": "jamie@webwizardsusa.com",
      "homepage": "https://webwizardsusa.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/http": "^10.0|^11.0",
    "illuminate/contracts": "^10.0|^11.0",
    "ext-dom": "*"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "pestphp/pest": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "autoload": {
    "psr-4": {
      "Webwizardsusa\\Larafeed\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webwizardsusa\\Larafeed\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Webwizardsusa\\Larafeed\\LarafeedServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
